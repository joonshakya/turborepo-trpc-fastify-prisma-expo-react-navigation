{
  "name": "@acme/server",
  "version": "0.1.0",
  "private": true,
  "main": "./index.ts",
  "types": "./index.ts",
  "license": "MIT",
  "scripts": {
    "build": "esbuild index.ts --bundle --packages=external --platform=node --format=cjs --outdir=dist --sourcemap",
    "dev": "tsx watch --clear-screen=false index",
    "dev:studio": "pnpm dlx prisma studio --browser none",
    "start": "esbuild index.ts --bundle --packages=external --platform=node --format=cjs --outdir=dist --sourcemap && node dist/index.js",
    "clean": "rm -rf .turbo node_modules",
    "lint": "eslint .",
    "format": "prettier --check \"**/*.{mjs,ts,md,json}\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@acme/db": "^0.1.0",
    "@fastify/websocket": "^7.1.2",
    "@trpc/server": "^10.41.0",
    "fastify": "^4.13.0",
    "jsonwebtoken": "^9.0.2",
    "superjson": "1.13.1",
    "tslib": "^2.5.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@acme/eslint-config": "^0.2.0",
    "@acme/prettier-config": "^0.1.0",
    "@acme/tsconfig": "^0.1.0",
    "@types/jsonwebtoken": "^9.0.4",
    "esbuild": "^0.17.10",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "tsx": "^3.12.7",
    "typescript": "^5.2.2"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@acme/eslint-config/base"
    ]
  },
  "prettier": "@acme/prettier-config"
}
